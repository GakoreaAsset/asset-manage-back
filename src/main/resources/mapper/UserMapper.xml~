<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kgav.gw.assetmanage.user.mapper.UserMapper">

    <!-- 아이디 및 비밀번호 조회  -->
    <select id="getUser" parameterType="UserModel" resultType="Integer">
        SELECT count(*)
          FROM userinfo
         WHERE userid=#{userid}
           AND userpw=#{userpw}
    </select>


</mapper>



